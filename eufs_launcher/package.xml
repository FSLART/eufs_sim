<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>eufs_launcher</name>
  <version>0.0.0</version>
  <description>The package that launches and links everything together</description>

  <maintainer email="e.bailey.andrew@gmail.com">Bailey Andrew</maintainer>
  <maintainer email="siliconlad@protonmail.com">Angus Stewart</maintainer>

  <license>MIT</license>

  <url type="website">http://eufs.co</url>
  <url type="repository">https://gitlab.com/eufs/eufs_sim</url>
  <url type="bugtracker">https://gitlab.com/eufs/eufs_sim/issues</url>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <build_depend>roslint</build_depend>

  <depend>gazebo_ros</depend>
  <depend>rclcpp</depend>
  <depend>rqt_gui_cpp</depend>
  <depend>eufs_gazebo</depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_python</build_type>
    <gazebo_ros plugin_path='${prefix}/lib' gazebo_media_path='${prefix}' />
    <rqt_gui plugin="${prefix}/plugin.xml"/>
  </export>
</package>
