<launch>

  <!-- Launch the simulation -->
  <include file="$(find eufs_gazebo)/launch/small_track.launch">
      <arg name="show_rqt_gui" default="false"/>
      <arg name="gui" value="false"/>
  </include>

  <!-- Launch the state_machine module -->
  <include file="$(find car_meta)/launch/state_machine.launch" >
      <arg name="launch_group" value="no_perception"/>
  </include>

  <test test-name="test_simulation" pkg="testing_sim" type="test_simulation.py" time-limit="60.0"/>
</launch>
