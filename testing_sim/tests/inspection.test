<launch>

  <arg name="launch_group" default="no_perception"/>
  <arg name="gui" default="true"/>

  <!-- Launch the simulation -->
  <include file="$(find eufs_gazebo)/launch/empty.launch">
      <arg name="show_rqt_gui" value="false"/>
      <arg name="gui" value="$(arg gui)"/>
  </include>

  <!-- Launch the state_machine module -->
  <include file="$(find car_meta)/launch/state_machine.launch" >
      <arg name="launch_group" value="$(arg launch_group)"/>
  </include>

  <!-- Run the autocross test (3 min time limit) -->
  <test test-name="test_inspection" pkg="testing_sim" type="test_inspection.py" time-limit="180.0"/>

</launch>
