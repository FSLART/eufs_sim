<?xml version="1.0"?>
<launch>

  <!-- start twist to ackermannDrive converter for robot steering plugin -->
  <node pkg="ros_can_sim" name="twist_to_chassisCommand" type="twist_to_chassisCommand.py" args="twistToAckermannDrive:=eufsa_control/teleop_vel_out" output="screen"/>
  
  <!-- start rqt robot steering module -->
  <node pkg="rqt_robot_steering" name="rqt_robot_steering" type="rqt_robot_steering" />
 
</launch>
